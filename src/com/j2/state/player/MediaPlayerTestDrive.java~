package com.j2.state.player;

public class MediaPlayerTestDrive {
 public static void main(String[] args) {
  MediaPlayer mp = new MediaPlayer("History of java");

  System.out.println(mp);

  mp.insertCD();
  mp.play();

  System.out.println(mp);

  mp.insertCD();
  mp.ejectCD();
  mp.diplaySleep();
  mp.diplayOff();
  System.out.println(mp);
 }
}